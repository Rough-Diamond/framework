<?xml version="1.0" encoding="UTF-8" standalone="no"?><mule xmlns="http://www.mulesource.org/schema/mule/core/2.1" xmlns:context="http://www.springframework.org/schema/context" xmlns:cxf="http://www.mulesource.org/schema/mule/cxf/2.1" xmlns:spring="http://www.springframework.org/schema/beans" xmlns:vm="http://www.mulesource.org/schema/mule/vm/2.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="                  http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-2.5.xsd                  http://www.mulesource.org/schema/mule/cxf/2.1 http://www.mulesource.org/schema/mule/cxf/2.1/mule-cxf.xsd                  http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.1.xsd                  http://www.mulesource.org/schema/mule/core/2.1 http://www.mulesource.org/schema/mule/core/2.1/mule.xsd                  http://www.mulesource.org/schema/mule/vm/2.1 http://www.mulesource.org/schema/mule/vm/2.1/mule-vm.xsd                  http://www.mulesource.org/schema/mule/servlet/2.1 http://www.mulesource.org/schema/mule/servlet/2.1/mule-servlet.xsd">

  <context:property-placeholder location="cxfrouting.properties"/>


  <model name="MonoOperationServiceConnectorModel">
    <service name="MonoOperationService_getServiceTypes">
      <inbound>
        <vm:inbound-endpoint path="SpaklServiceConnector_getServiceTypesIn">
          <custom-transformer class="jp.rough_diamond.spakl.esb.service.stub.spaklservice.ObjectToJAXBElement" name="SpaklServiceConnector_getServiceTypesInTransformer"/>
        </vm:inbound-endpoint>
      </inbound>
      <bridge-component/>
      <outbound>
        <chaining-router>
          <cxf:outbound-endpoint address="http://${server.host}:${server.port}/services/SpaklService" clientClass="jp.rough_diamond.spakl.esb.service.stub.spaklservice.SpaklService" mtomEnabled="true" operation="getServiceTypes" wsdlLocation="file:///${wsdl.dir}/SpaklService.wsdl" wsdlPort="SpaklServicePort"/>
        </chaining-router>
      </outbound>
    </service>
    <service name="MonoOperationService_getServiceParameters">
      <inbound>
        <vm:inbound-endpoint path="SpaklServiceConnector_getServiceParametersIn">
          <custom-transformer class="jp.rough_diamond.spakl.esb.service.stub.spaklservice.ObjectToJAXBElement" name="SpaklServiceConnector_getServiceParametersInTransformer"/>
        </vm:inbound-endpoint>
      </inbound>
      <bridge-component/>
      <outbound>
        <chaining-router>
          <cxf:outbound-endpoint address="http://${server.host}:${server.port}/services/SpaklService" clientClass="jp.rough_diamond.spakl.esb.service.stub.spaklservice.SpaklService" mtomEnabled="true" operation="getServiceParameters" wsdlLocation="file:///${wsdl.dir}/SpaklService.wsdl" wsdlPort="SpaklServicePort"/>
        </chaining-router>
      </outbound>
    </service>
  </model>
</mule>